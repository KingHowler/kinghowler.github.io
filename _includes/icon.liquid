<img
  {% if include.icon.site %}
    {% if include.icon.site == "devicons" %}
      src="https://cdn.jsdelivr.net/gh/devicons/devicon/icons/{{ include.icon.link }}"
    {% elsif include.icon.site == "skillicons" %}
      src="https://skillicons.dev/icons?i={{ include.icon.link }}"
    {% endif %}
  {% else %}
    src="{{ include.icon.link }}"
  {% endif %}

  {% if include.alt %}
    alt="{{ include.alt }}"
  {% endif %}
  {% if include.title %}
    title="{{ include.title }}"
  {% endif %}
  {% if include.width %}
    width="{{ include.width }}"
  {% else %}
    width="auto"
  {% endif %}
  {% if include.height %}
    height="{{ include.height }}"
  {% else %}
    height="40"
  {% endif %}
>
