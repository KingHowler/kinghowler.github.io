{
  "files.exclude": {
    "**/_data/": true,
    "**/_includes/": true,
    "**/_layouts/": true,
    "**/_plugins/": true,
    "**/_sass/": true,
    "**/.devcontainer/": true,
    "**/.github/": true,
    "**/bin/": true,
    "**/lighthouse_results/": true,
    "**/readme_preview/": true,
    "**/.all-contributorsrc": true,
